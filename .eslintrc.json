{
    "env": {
        "node": true,
        "commonjs": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "semi": ["warn", "always"],
        "quotes": ["warn", "single", { "allowTemplateLiterals": true }],
        "no-unused-vars": "warn"
    }
}
